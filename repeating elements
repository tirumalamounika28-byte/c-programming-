#include <stdio.h>
int main() {
    int arr[100], n, i, j;
    int found = 0;
printf("Enter number of elements: ");
    scanf("%d", &n);
printf("Enter elements:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
  printf("Repeating elements are:\n");
    for (i = 0; i < n; i++) {
        for (j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                printf("%d\n", arr[i]);
                found = 1;
                break;
            }
        }
    }

    if (!found) {
        printf("No repeating elements\n");
    }

    return 0;
}
