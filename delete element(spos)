#include<stdio.h>
int main()
{
int n,a[10],pos,i;
printf("Enter size of an arary");
scanf("%d",&n);
printf("Enter array elements");
for(i=0;i<=n;i++)
scanf("%d",&a[i]);
printf("enter the position");
scanf("%d",&pos);
n--;
for(i=pos;i<=n;i++)
a[i-1]=a[i];
printf("After deleting array elements are");
return 0;
}
