#include <stdio.h>
#include <math.h>

int main()
{
float a, b, c, r1, r2, d;
printf("Enter a, b, c values: ");
scanf("%f %f %f", &a, &b, &c);
d = (b * b) - (4 * a * c);
if (d == 0)
{
printf("Roots are real and equal\n");
r1 = -b / (2 * a);
r2 = r1;
printf("Root1 = %f and Root2 = %f", r1, r2);
}
else if (d > 0)
{
printf("Roots are real and different\n");
r1 = (-b + sqrt(d)) / (2 * a);
r2 = (-b - sqrt(d)) / (2 * a);
printf("Root1 = %f\n", r1);
printf("Root2 = %f", r2);
}
else
{
printf("Roots are imaginary");
}
return 0;
}
